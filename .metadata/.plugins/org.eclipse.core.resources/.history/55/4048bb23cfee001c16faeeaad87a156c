	static int[] dijkstra(int V, ArrayList<ArrayList<ArrayList<Integer>>> adj, int S) {
    
		Comparator<DNode> comparator = (dn1, dn2) -> dn1.dist - dn2.dist;
	}

}

class DNode {
	int dist, value;
	
	public DNode(int dist, int value) {
		this.dist = dist;
		this.value = value;
	}
}
